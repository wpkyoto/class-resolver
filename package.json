{
  "name": "class-resolver",
  "version": "2.2.0",
  "description": "Simple class resolver.",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.mjs"
      },
      "require": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "vite build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "prepublishOnly": "npm run build",
    "release": "np"
  },
  "keywords": ["resolver", "chain-of-responsibility", "typescript"],
  "author": "Hidetaka Okamoto <info@wp-kyoto.net> (https://wp-kyoto.net)",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.0.0",
    "@vitest/coverage-v8": "^4.0.9",
    "@vitest/ui": "^4.0.9",
    "np": "^10.2.0",
    "typescript": "^5.7.2",
    "vite": "^7.2.2",
    "vite-plugin-dts": "^4.3.0",
    "vitest": "^4.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hideokamoto/class-resolver.git"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "np": {
    "yarn": false,
    "contents": "dist"
  }
}
